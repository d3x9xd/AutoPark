<Window x:Class="AutoPark.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoPark"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">




    <Grid>
        <StackPanel
            Background="#FFFFFF" />

        <StackPanel
            Orientation="Horizontal"
            HorizontalAlignment="Center"/>

        <Frame
            Name="FrmMain"
            NavigationUIVisibility="Hidden"
            Grid.Row="1"/>

        <TextBlock
            FontSize="18pt"
            FontWeight="Bold"
            Margin="396,10,396,0"
            VerticalAlignment="Top"
            HorizontalAlignment="Center"
            Text="{Binding ElementName=FrmMain, Path=Content.Title}" Height="127"
            />
        <Image Source="./resources/Logo.png" Width="73" VerticalAlignment="Top" HorizontalAlignment="Left" Height="70"/>


    </Grid>
</Window>
